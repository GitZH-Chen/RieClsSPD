
#LogEigMLR
dataset=RADAR
dataset.path=/data/zchen/Datasets/Radar/Python/
fit.epochs=5
nnet.model.architecture=[20,16,14,12,10,8]
nnet.model.classifier=LogEigMLR
hydra.run.dir=outputs/debug
hydra.sweep.dir=outputs/debug

#LCM
dataset=RADAR
dataset.path=/data/zchen/Datasets/Radar/Python/
fit.epochs=5
nnet.model.architecture=[20,16,14,12,10,8]
nnet.model.classifier=SPDMLR
nnet.model.metric=SPDLogCholeskyMetric
nnet.model.power=0.5
hydra.run.dir=outputs/debug
hydra.sweep.dir=outputs/debug

dataset=RADAR
dataset.path=/data/zchen/Datasets/Radar/Python/
fit.epochs=5
